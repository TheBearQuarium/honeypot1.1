{
  "extends": "eslint-config-airbnb",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {

    "no-console": 0,
    "prefer-template": 0,
    "no-undef": 0,
    "func-names": 0,
    "prefer-arrow-callback": 0,
    "quote-props": 0,
    "one-var": 0,
    "arrow-body-style": 0,
    "no-unused-vars": 0,
    "no-use-before-define": 0,

  }
}
